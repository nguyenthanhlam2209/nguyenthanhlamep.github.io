<script>
var callBack = function (variant, selector) {
	if (variant) {
		modal = jQuery('#quick-view-modal');
		jQuery('.p-price').html(Haravan.formatMoney(variant.price, "{{amount}}₫"));
		if (variant.compare_at_price > 0)
			modal.find('del').html(Haravan.formatMoney(variant.compare_at_price, "{{amount}}₫"));
		else
			modal.find('del').html('');
		if (variant.available) {
			modal.find('.btn-addcart').css('display', 'block');
			modal.find('.btn-soldout').css('display', 'none');
		}
		else {
			modal.find('.btn-addcart').css('display', 'none');
			modal.find('.btn-soldout').css('display', 'block');
		}
	}
	else {
		modal.find('.btn-addcart').css('display', 'none');
		modal.find('.btn-soldout').css('display', 'block');
	}
}
var p_select_data =jQuery('.p-option-wrapper').html();
var p_zoom = jQuery('.image-zoom').html();
var quickViewProduct = function (purl) {

	if (jQuery(window).width() < 680) { window.location = purl; return false; }
	modal = jQuery('#quick-view-modal'); modal.modal('show');
	jQuery.ajax({
		url: purl + '.js',
		async: false,
		success: function (product) {
			jQuery.each(product.options, function (i, v) {
				product.options[i] = v.name;
			})
			modal.find('.p-title').html(product.title);
			modal.find('.p-option-wrapper').html(p_select_data);
			jQuery('.image-zoom').html(p_zoom);
			modal.find('.p-url').attr('href', product.url);
			if (product.images.length == 0) {
				modal.find('.p-product-image-feature').attr('src', '//hstatic.net/0/0/global/noDefaultImage6_large.gif');
			}
			else {
				jQuery('#p-sliderproduct').remove();
				jQuery('.image-zoom').append("<div id='p-sliderproduct' class='flexslider'>");
				jQuery('#p-sliderproduct').append("<ul class='slides'>");

				jQuery.each(product.images, function (i, v) {
					elem =  jQuery('<li class="product-thumb">').append('<a href="#" data-image="" data-zoom-image=""><img /></a>');
					elem.find('a').attr('data-image', Haravan.resizeImage(v, 'medium'));
					elem.find('a').attr('data-zoom-image', v);
					elem.find('img').attr('data-image', Haravan.resizeImage(v, 'medium'));
					elem.find('img').attr('data-zoom-image',v);
					elem.find('img').attr('src', Haravan.resizeImage(v, 'small'));
					modal.find('.slides').append(elem);
				})

				modal.find('.p-product-image-feature').attr('src', product.featured_image);
				jQuery(".modal-footer .btn-readmore").attr('href', purl);
				var iflag = 0;
				jQuery('#p-sliderproduct img').load(function () {
					iflag++;
					if (iflag ==  jQuery('#p-sliderproduct img').length) {
						jQuery('#p-sliderproduct').flexslider({
							animation: "slide",
							controlNav: false,
							animationLoop: false,
							slideshow: false,
							itemWidth: 115
						});
					}
				})
				modal.find('.owl-item:first-child img').click();
			}
			jQuery.each(product.variants, function (i, v) {
				modal.find('select#p-select').append("<option value='" + v.id + "'>" + v.title + ' - ' + v.price + "</option>");
			})
			if (product.variants.length == 1 && product.variants[0].title.indexOf('Default') != -1)
				jQuery('.p-option-wrapper').hide();
			else
				jQuery('.p-option-wrapper').show();
			if (product.variants.length == 1 && product.variants[0].title.indexOf('Default') != -1) {
				callBack(product.variants[0], null);
			}
			else {
				new Haravan.OptionSelectors("p-select", { product: product, onVariantSelected: callBack });
				debugger
				if (product.options.length == 1 && product.options[0].indexOf('Tiêu đề')==-1)

					modal.find('.selector-wrapper:eq(0)').prepend('<label>' + product.options[0] + '</label>');

				jQuery('.p-option-wrapper select:not(#p-select)').each(function () {
					jQuery(this).wrap('<span class="custom-dropdown custom-dropdown--white"></span>');
					jQuery(this).addClass("custom-dropdown__select custom-dropdown__select--white");
				});
				callBack(product.variants[0], null);
			}
		}
	});

	//$('.modal-backdrop').css('opacity', '0');
	return false;
}
jQuery('.read-more.p-url').click(function(event) {
        event.preventDefault();
            window.top.location = jQuery(this).attr('href');
        return false;
            });
jQuery('#quick-view-modal ').on('click', '.product-thumb img', function (event) {
	event.preventDefault();
	modal =  jQuery('#quick-view-modal');
	modal.find('.p-product-image-feature').attr('src',  jQuery(this).attr('data-zoom-image'));
	modal.find('.product-thumb').removeClass('active');
	jQuery(this).parents('li').addClass('active');
	return false;
})
jQuery(document).ready(function () {
	jQuery('.mask-view').click(function (event) {
		event.preventDefault();
		quickViewProduct(jQuery(this).attr('data-handle'));
	})
})
